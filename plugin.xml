        <pluginName>AssetUnicodeName</pluginName>

        <pluginDescription>
            Plugin AssetUnicodeName was created to remember original name of the asset (e.g. special letters in German).
        </pluginDescription>

        <pluginVersion>1.0</pluginVersion>
        <pluginRevision>1</pluginRevision>
        <pluginBuildTimestamp>0</pluginBuildTimestamp>

        <pluginClassName>\AssetUnicodeName\Plugin</pluginClassName>

        <pluginIncludePaths>
